   "source": [
    "# Basci Recursion Example\n",
    "\n",
    "### Factorial Numbers\n",
    "#### f(n) = n x f(n-1)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 3,
   "metadata": {},
   "outputs": [],
   "source": [
    "\n",
    "\n",
    "def f(n):\n",
    "    result = 1\n",
    "    if n > 1:\n",
    "        result = n * f(n-1)\n",
    "    return result\n",
    "\n",
    "f(4)"
   ]
  }
